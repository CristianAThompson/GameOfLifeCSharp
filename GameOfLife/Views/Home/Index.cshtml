@using GameOfLife.Models
@{
    ViewBag.Title = "Home Page";
}

@foreach (Cell cell in Model.grid)
{
    if (cell.state)
    {
        <div style="height: 40px; width: 40px; background: green; display: inline-block; margin-bottom: 2px;">@cell.state</div>
    }
    else
    {
        <div style="height: 40px; width: 40px; background: red; display: inline-block; margin-bottom: 2px;">@cell.state</div>
    }
}
